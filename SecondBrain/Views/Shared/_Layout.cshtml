@{
    ViewData["typeDisplay"] = 0;
}
@{
    DateOnly today;
    string FullDate;
    Console.WriteLine(TempData["CurrentDate"].ToString());
    if (1 == 1)
    {
        today = DateOnly.FromDateTime(DateTime.Now);
        string fullDay = today.Year.ToString() + "-" + (today.Month < 10 ? "0" + (today.Month).ToString() : (today.Month).ToString()) + "-" + (today.Day < 10 ? "0" + today.Day.ToString() : today.Day.ToString());
        TempData["CurrentDate"] = fullDay;
        TempData["FullDate"] = today.ToString();
    } else
    {
        FullDate = TempData["FullDate"]?.ToString();
        today = DateOnly.Parse(FullDate);
    }
}
@using System.Globalization

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
